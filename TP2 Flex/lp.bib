@inproceedings{citeulike:4667726,
    abstract = {{ion with Multiple Jargons We tend to think of systems as coded in a single language such as C or COBOL, but the reality may be quite different Software systems, especially as they grow in complexity, are often constructed using many different technologies and associated DSLs as building blocks An example is a WWW application built with CGI programming that involves the HTTP protocol, HTML, a shell language such as KornShell, the CGI protocol, and a programming language such as Perl The application may also require Java, JavaScript, and SQL And these are just the languages with names An application may use an assortment of nameless, adhoc "data formats" for representing its inputs and outputs Additional artifacts such as documentation, test suites, and persistent data may also require formal representations So we see that a complex domain such an interactive Web applications is actually a collection of subdomains, each addressed by a different DSL, some given, some used by choic}},
    author = {Nakatani, Lloyd H and Jones, Mark A},
    booktitle = {In First ACM SIGPLAN Workshop on DomainSpecific Languages},
    citeulikearticleid = {4667726},
    citeulikelinkout0 = {http://citeseerxistpsuedu/viewdoc/summary?doi=101151858},
    keywords = {compilers, dsl},
    pages = {5974},
    postedat = {20090528 16:43:01},
    priority = {2},
    title = {{Jargons and Infocentrism}},
    url = {http://citeseerxistpsuedu/viewdoc/summary?doi=101151858},
    year = {1997}
}

@inproceedings{kephera,
    abstract = {{The KHEPERA system is a toolkit for the rapid implementation and longterm maintenance of domain specific languages (DSLs) Our viewpoint is that DSLs are most easily implemented via sourcetosource translation from the DSL into another language and that this translation should be based on simple parsing, sophisticated treebased analysis and manipulation, and source generation using prettyprinting techniques KHEPERA emphasizes the use of familiar, preexisting tools and provides support for transformation replay and debugging for the DSL processor and enduser programs In this paper, we present an overview of our approach, including implementation details and a short example}},
